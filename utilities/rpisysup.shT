		echo "$1" > /dev/console
	;;
	logger) #[ -n "$LOGCHECKS" ] && echo "$i-$SL> ${1}" >> $iL
		logger -p info -t $i "${*}"
	;;
	*) $ecmd "$1"; ;;
esac
}

