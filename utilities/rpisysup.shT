echL() { SL=$(date +%Y%m%d%H%M%S)

### all cases except * had log anyway move above
[ -n "$LOGCHECKS" ] && echo "$i-$SL> ${1}" >> $iL

case "${2}" in
	status) 
		echo "#${1}" >> "$FLASHSTATUSf" #experimental >> here > msgout sysuponline
	;;
